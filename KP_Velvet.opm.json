{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#0E050F",
          "foreground": "#EFDCF9",
          "properties": {
            "arch": "\uf303",
            "debian": "\uf306",
            "fedora": "\uf30a",
            "linux": "\uf17c",
            "macos": "\uf179",
            "manjaro": "\uf312",
            "opensuse": "\uf314",
            "ubuntu": "\uf31b",
            "windows": "\uf17a"
          },
          "style": "powerline",
          "template": " {{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#170B3B",
          "foreground": "#EFDCF9",
          "powerline_symbol": "\ue0b4",
          "properties": {
            "home_icon": "~",
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#341948",
          "foreground": "#EFDCF9",
          "powerline_symbol": "\ue0b4",
          "properties": {
            "style": "hostname"
          },
          "style": "powerline",
          "template": " {{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#4c1f5e",
          "foreground": "#EFDCF9",
          "properties": {
            "time_format": "15:04"
          },
          "style": "powerline",
          "template": " {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        },
        {
          "background": "#69307A",
          "foreground": "#EFDCF9",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF3C3C{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " \uf08a{{ if gt .Code 0 }} {{.Code}}{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "type": "text",
      "style": "plain",
      "foreground": "#EFDCF9",
      "background": "#4c1f5e",
      "template": "\n ï‚Š"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#4c1f5e",
          "foreground": "#E4F34A",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "python"
        },
        {
          "background": "#4c1f5e",
          "foreground": "#7FD5EA",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "node"
        },
        {
          "background": "#4c1f5e",
          "foreground": "#E64747",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "java"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .Shell }} - {{ .Folder }}",
  "final_space": true,
  "version": 3
}
